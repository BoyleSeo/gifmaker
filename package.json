{
  "homepage": "https://boyleseo.github.io/gifmaker/",
  "name": "boyle",
  "version": "0.1.0",
  "scripts": {
    "start": "yarn build:css && snowpack dev",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "build": "snowpack build && touch build/.nojekyll",
    "test": "web-test-runner \"src/**/*.test.jsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx}\"",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.8.4",
    "@ffmpeg/ffmpeg": "^0.9.5",
    "autoprefixer": "^10.0.2",
    "gh-pages": "^3.1.0",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "rc-slider": "^9.6.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-image-crop": "^8.6.6",
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.0.4",
    "@snowpack/plugin-react-refresh": "^2.3.7",
    "@snowpack/web-test-runner-plugin": "^0.1.4",
    "@testing-library/react": "^11.0.0",
    "@web/test-runner": "^0.9.7",
    "chai": "^4.2.0",
    "prettier": "^2.0.5",
    "snowpack": "^2.17.1"
  }
}
